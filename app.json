{
  "expo": {
    "name": "Rhythm Dodge Game",
    "slug": "rhythm-dodge-game",
    "version": "1.0.0",
    "orientation": "landscape",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1a1a1a"
      },
      "orientationLock": "landscape"
    },
    "web": {
      "bundler": "metro"
    }
  }
}